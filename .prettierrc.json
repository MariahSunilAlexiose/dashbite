{
  "trailingComma": "es5",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": false,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^react$",
    "^(react-router-dom/(.*)$)|^(react-router-dom$)",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^(@context|@cmp|@|@img)(/.*)$",
    "",
    "^\\.\\./.*$",
    "",
    "^[.]",
    "",
    ".*\\.css$"
  ]
}
